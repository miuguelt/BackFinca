    "editor.tokenColorCustomizations": {
        "comments": "#7C8597",
        "strings": "#9BD69A",
        "keywords": "#4CC6FF",
        "functions": "#E6E1A3",
        "numbers": "#FFD580",
        "types": "#FFD580",
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#7C8597",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "string",
                    "punctuation.definition.string"
                ],
                "settings": {
                    "foreground": "#9BD69A"
                }
            },
            {
                "scope": [
                    "keyword",
                    "storage.type"
                ],
                "settings": {
                    "foreground": "#4CC6FF",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "entity.name.function",
                    "support.function"
                ],
                "settings": {
                    "foreground": "#E6E1A3"
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.language"
                ],
                "settings": {
                    "foreground": "#FFD580"
                }
            }
        ]
    },
    "editor.fontFamily": "'Fira Code', 'JetBrains Mono', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "'Fira Code', 'JetBrains Mono', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "terminal.background": "#071826",
    "terminal.foreground": "#F0F4F8",
    "terminalCursor.foreground": "#0A84FF",

    "window.titleBarStyle": "custom",
    "editor.fontSize": 15,
    "terminal.integrated.fontSize": 13,
    "chat.editor.fontSize": 20,
    "window.zoomLevel": 2
}